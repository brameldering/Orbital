{
  "name": "orbital-backend",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "eCommerce application built with the MERN stack",
  "main": "server.ts",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "server": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
